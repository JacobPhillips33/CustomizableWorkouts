@model IEnumerable<Exercise>

<table class="table">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Body Part</th>
        <th>Target Muscle</th>
        <th>Equipment Needed</th>
    </tr>

    @foreach (var exercise in @Model)
    {
        <tr>
            <td><a href=/Exercise/ViewExercise/@exercise.ExerciseNumber>@exercise.ExerciseNumber</a></td>
            <td>@exercise.Name</td>
            <td>@exercise.BodyPart</td>
            <td>@exercise.TargetMuscle</td>
            <td>@exercise.Equipment</td>
        </tr>
    }

</table>